#!ipxe
#
# vSphere 7.0b
#
# The $base_url is where the boot.cfg and iso contents live
# The mboot.c32 file is in the iso folder
set base_url http://${http_ip}:${http_port}/vsphere/7.0b
#
#
kernel ${base_url}/iso/mboot.c32 -c ${base_url}/boot.cfg BOOTIF=${netX/mac} runweasel ks=http://${http_ip}:${http_port}/vsphere/7.0b/ks.cfg
boot
